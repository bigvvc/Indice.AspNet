<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>Indice.AspNetCore.Identity Class Library</Description>
    <Copyright>Copyright (c) 2018 Indice</Copyright>
    <AssemblyTitle>Indice.AspNetCore.Identity</AssemblyTitle>
    <VersionPrefix>1.2.11</VersionPrefix>
    <!--<VersionSuffix>beta1</VersionSuffix>-->
    <Authors>c.leftheris, g.manoltzas</Authors>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>Indice.AspNetCore.Identity</AssemblyName>
    <AssemblyTitle>Indice.AspNetCore.Identity</AssemblyTitle>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>
  <PropertyGroup Label="Package">
    <PackageId>Indice.AspNetCore.Identity</PackageId>
    <PackageTags>AspNetCore;IdentityServer;</PackageTags>
    <PackageReleaseNotes>
      Enhance profile service.
    </PackageReleaseNotes>
    <PackageIconUrl>https://raw.githubusercontent.com/indice-co/Indice.AspNet/master/icon/icon-128.png</PackageIconUrl>
    <PackageProjectUrl>https://github.com/indice-co/Indice.AspNet</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/indice-co/Indice.AspNet/blob/master/LICENSE</PackageLicenseUrl>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/indice-co/Indice.AspNet</RepositoryUrl>
    <Company>Indice</Company>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="IdentityServer4" Version="2.2.0" />
    <PackageReference Include="IdentityServer4.AccessTokenValidation" Version="2.6.0" />
    <PackageReference Include="IdentityServer4.EntityFramework" Version="2.1.0" />
    <PackageReference Include="Indice.AspNetCore" Version="1.2.9" />
    <PackageReference Include="Microsoft.AspNetCore.Identity" Version="2.1.0" />
    <PackageReference Include="Microsoft.AspNetCore.Identity.EntityFrameworkCore" Version="2.1.0" />
  </ItemGroup>
</Project>
